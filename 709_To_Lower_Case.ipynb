        "s = \"Hello\"\n",
        "sol = ''\n",
        "for w in s:\n",
        "  if w.upper():\n",
        "    sol += w.lower()\n",
        "  else:\n",
        "    sol += w\n",
        "\n",
        "\n",
        "print(sol)"
      ],
